{
	"name": "sample-biomejs-ci",
	"version": "1.0.0",
	"scripts": {
		"build": "tsc --build",
		"format": "biome format",
		"lint": "biome lint",
		"lint:staged": "biome lint --staged",
		"postinstall": "git config core.hooksPath ./.githooks",
		"prestart": "pnpm build",
		"start": "node ./dist/index.js",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"type-check": "tsc --noEmit --incremental false"
	},
	"license": "MIT",
	"packageManager": "pnpm@10.12.4",
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4"
	}
}
